    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "aws-cdk-lib": "~2.224",
    "cdk-monitoring-constructs": "^9.16.0",
    "constructs": "^10.4.2"
  },
  "overrides": {
    "js-yaml": "^4.1.1"
  }
}
